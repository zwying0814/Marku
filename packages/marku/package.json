{
  "name": "marku",
  "version": "1.0.2",
  "description": "A lightweight and flexible counter library for web applications, browser-ready UMD package",
  "main": "dist/index.umd.js",
  "browser": "dist/index.umd.js",
  "umd:main": "dist/index.umd.js",
  "unpkg": "dist/index.umd.min.js",
  "jsdelivr": "dist/index.umd.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*.js",
    "dist/*.js.map",
    "dist/*.d.ts",
    "dist/*.d.ts.map",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "build:dev": "rollup -c --environment NODE_ENV:development",
    "build:prod": "rollup -c --environment NODE_ENV:production",
    "dev": "node scripts/dev.js",
    "watch": "rollup -c --watch",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "npx http-server -p 5500",
    "npm:version:patch": "npm version patch",
    "npm:version:minor": "npm version minor",
    "npm:version:major": "npm version major"
  },
  "keywords": [
    "counter",
    "web",
    "frontend",
    "browser",
    "umd",
    "cdn",
    "javascript",
    "typescript",
    "library",
    "marku",
    "unpkg",
    "jsdelivr"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/marku.git",
    "directory": "packages/marku"
  },
  "homepage": "https://github.com/your-username/marku#readme",
  "bugs": {
    "url": "https://github.com/your-username/marku/issues"
  },
  "packageManager": "pnpm@10.16.1",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.4.0",
    "chokidar": "^4.0.3",
    "http-server": "^14.1.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.1",
    "tslib": "^2.8.1",
    "typescript": "~5.9.2"
  },
  "peerDependencies": {},
  "dependencies": {}
}
